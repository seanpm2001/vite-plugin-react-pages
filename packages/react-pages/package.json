{
  "name": "vite-plugin-react-pages",
  "version": "3.2.1",
  "main": "dist/node/index.js",
  "module": "dist/node-esm/index.mjs",
  "type": "commonjs",
  "exports": {
    ".": {
      "import": "./dist/node-esm/index.mjs",
      "require": "./dist/node/index.js"
    },
    "./*": "./*"
  },
  "bin": {
    "vite-pages": "bin/vite-pages.js"
  },
  "keywords": [
    "vite",
    "react",
    "vite-pages",
    "react-pages",
    "vite-plugin-react-pages"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vitejs/vite-plugin-react-pages.git"
  },
  "bugs": {
    "url": "https://github.com/vitejs/vite-plugin-react-pages/issues"
  },
  "homepage": "https://github.com/vitejs/vite-plugin-react-pages",
  "scripts": {
    "build": "rimraf ./dist && concurrently \"npm:build-*\"",
    "build-node": "tsc -p src/node",
    "deprecated-build-node-esm": "tsc -p src/node --module ES2022 --outDir ./dist/node-esm && node ./scripts/addESMDeclare.mjs",
    "build-node-esm": "rollup -c",
    "build-client": "tsc -p src/client",
    "dev": "rimraf ./dist && concurrently \"npm:dev-*\"",
    "dev-node": "tsc -w -p src/node",
    "dev-client": "tsc -w -p src/client",
    "prepare": "npm run build"
  },
  "peerDependencies": {
    "vite": "^2.5.6 || ^3.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-typescript": "^8.3.4",
    "@babel/preset-env": "^7.18.10",
    "@types/enhanced-resolve": "^3.0.6",
    "@types/fs-extra": "^9.0.8",
    "@types/minimist": "^1.2.0",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@types/react-router-dom": "^5.1.7",
    "concurrently": "^7.3.0",
    "react": "^17.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.78.0",
    "typescript": "^4.3.2",
    "vite": "^3.0.7",
    "vite-plugin-mdx": "^3.5.6"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "chokidar": "^3.5.1",
    "dequal": "^2.0.3",
    "enhanced-resolve": "^5.10.0",
    "fs-extra": "^10.1.0",
    "gray-matter": "^4.0.2",
    "jest-docblock": "^28.1.1",
    "jotai": "^0.15.3",
    "mini-debounce": "^1.0.8",
    "minimist": "^1.2.5",
    "remark-frontmatter": "^2.0.0",
    "slash": "^3.0.0",
    "tiny-invariant": "^1.1.0",
    "typescript": "^4.7.4",
    "unist-util-visit": "^2.0.3"
  }
}
